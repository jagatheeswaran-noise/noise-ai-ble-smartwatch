<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/cvLog"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:padding="16dp"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/llLog"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>

    </ScrollView>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cvLog">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="50dp">

            <Button
                android:id="@+id/btnConnect"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/s2" />

            <Button
                android:id="@+id/btnDisconnect"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/s327" />

            <LinearLayout
                android:id="@+id/berryG1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#ff0000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PRIVATE"
                    android:textColor="#ff0000" />

                <Button
                    android:id="@+id/btnGetProtocol"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s576" />

                <androidx.appcompat.widget.AppCompatEditText
                    android:id="@+id/etUserId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:hint="@string/s578" />

                <Button
                    android:id="@+id/btnSetUserId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s577" />

                <Button
                    android:id="@+id/btnBind"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s3" />

                <Button
                    android:id="@+id/btnBindSuc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s592" />

                <Button
                    android:id="@+id/btnOneKeyBind"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s609" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#ff0000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="PROTOBUF"
                    android:textColor="#ff0000" />

                <Button
                    android:id="@+id/btnGetDeviceInfo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s8" />

                <Button
                    android:id="@+id/btnUnbind"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s4" />


                <Button
                    android:id="@+id/btnReqAgps"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s579" />

                <Button
                    android:id="@+id/btnSendAgps2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s582"
                    android:visibility="gone" />

                <Button
                    android:id="@+id/btnSendAgps"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s580" />

            </LinearLayout>

            <RadioGroup
                android:id="@+id/rgLog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RadioButton
                    android:id="@+id/rb1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/s584" />

                <RadioButton
                    android:id="@+id/rb2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/s585" />

                <RadioButton
                    android:id="@+id/rb3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/s586" />

                <RadioButton
                    android:id="@+id/rb4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/s587" />

                <RadioButton
                    android:id="@+id/rb5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/s588" />

                <RadioButton
                    android:id="@+id/rb6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/s589" />
            </RadioGroup>

            <Button
                android:id="@+id/btnReqLog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/s583" />

            <Button
                android:id="@+id/btnStartLog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/s590" />

            <Button
                android:id="@+id/btnEndLog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/s591" />

            <LinearLayout
                android:id="@+id/berryG2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvTip1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:autoLink="web" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp" />

                <Button
                    android:id="@+id/btnGetOtaFile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="@string/s594" />

                <Button
                    android:id="@+id/btnOta"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s593" />

                <Button
                    android:id="@+id/btnSendAppN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s86"
                    android:textAllCaps="false" />

                <EditText
                    android:id="@+id/etOtaLoop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/tv_count"
                    android:inputType="phone" />

                <Button
                    android:id="@+id/btnOta2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s624" />

                <Button
                    android:id="@+id/btnOta3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s625" />

                <Button
                    android:id="@+id/btnOta4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/s626" />

                <Button
                    android:id="@+id/btnDail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="@string/s23" />

                <Button
                    android:id="@+id/btnOtherSet"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="@string/s605" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/berryTest1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#ff0000" />

                <androidx.appcompat.widget.AppCompatEditText
                    android:id="@+id/etTestCmd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:hint="@string/s634" />

                <Button
                    android:id="@+id/btnTestCmd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="SEND" />

                <LinearLayout
                    android:id="@+id/llTestLog"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="150dp"
                    android:orientation="vertical">

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>